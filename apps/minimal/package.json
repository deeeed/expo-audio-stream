{
    "name": "minimal",
    "main": "src/index",
    "version": "1.0.1",
    "scripts": {
        "build:deps": "cd ../../packages/expo-audio-studio && yarn build && yarn build:plugin",
        "start": "yarn build:deps && EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=development expo start --dev-client",
        "android": "expo run:android",
        "ios": "expo run:ios --device",
        "ios:sim": "expo run:ios",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "lint": "expo lint",
        "publish": "bash scripts/deploy-minimal.sh",
        "deploy": "NODE_ENV=production expo export -p web && gh-pages -t -d dist --dest minimal"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@expo/vector-icons": "^15.0.3",
        "@react-navigation/native": "^7.0.0",
        "@siteed/expo-audio-studio": "workspace:^",
        "@siteed/expo-audio-ui": "workspace:^",
        "@siteed/react-native-logger": "^0.10.0",
        "expo": "^54.0.0",
        "expo-audio": "~1.1.1",
        "expo-constants": "~18.0.13",
        "expo-font": "~14.0.11",
        "expo-haptics": "~15.0.8",
        "expo-linking": "~8.0.11",
        "expo-router": "~6.0.23",
        "expo-splash-screen": "~31.0.13",
        "expo-status-bar": "~3.0.9",
        "expo-symbols": "~1.0.8",
        "expo-system-ui": "~6.0.9",
        "expo-web-browser": "~15.0.10",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-native": "0.81.5",
        "react-native-gesture-handler": "~2.28.0",
        "react-native-reanimated": "~4.1.1",
        "react-native-safe-area-context": "~5.6.0",
        "react-native-screens": "~4.16.0",
        "react-native-web": "^0.21.0",
        "react-native-worklets": "0.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@react-native-community/cli": "^16.0.0",
        "@types/gh-pages": "^6",
        "@types/jest": "^29.5.12",
        "@types/react": "~19.1.10",
        "@types/react-test-renderer": "^18.0.7",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-universe": "^12.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.34.1",
        "gh-pages": "^6.2.0",
        "jest": "^29.2.1",
        "jest-expo": "~54.0.17",
        "prettier": "^3.2.5",
        "react-test-renderer": "18.3.1",
        "typescript": "~5.9.2"
    },
    "private": true,
    "expo": {
        "autolinking": {
            "nativeModulesDir": "../packages/"
        }
    }
}
