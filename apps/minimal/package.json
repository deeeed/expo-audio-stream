{
    "name": "minimal",
    "main": "src/index",
    "version": "1.0.0",
    "scripts": {
        "build:deps": "cd ../../packages/expo-audio-studio && yarn build && yarn build:plugin",
        "start": "yarn build:deps && EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=development expo start --dev-client",
        "android": "expo run:android",
        "ios": "expo run:ios --device",
        "web": "expo start --web",
        "test": "jest --watchAll",
        "lint": "expo lint",
        "publish": "bash scripts/deploy-minimal.sh",
        "deploy": "NODE_ENV=production expo export -p web && gh-pages -t -d dist --dest minimal"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "dependencies": {
        "@expo/vector-icons": "^14.0.2",
        "@react-navigation/native": "^7.0.0",
        "@siteed/expo-audio-studio": "workspace:^",
        "@siteed/expo-audio-ui": "workspace:^",
        "@siteed/react-native-logger": "^0.10.0",
        "expo": "^53.0.20",
        "expo-audio": "~0.4.8",
        "expo-constants": "~17.1.7",
        "expo-font": "~13.3.2",
        "expo-haptics": "~14.1.4",
        "expo-linking": "~7.1.7",
        "expo-router": "~5.1.4",
        "expo-splash-screen": "~0.30.10",
        "expo-status-bar": "~2.2.3",
        "expo-symbols": "~0.4.5",
        "expo-system-ui": "~5.0.10",
        "expo-web-browser": "~14.2.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-native": "0.79.5",
        "react-native-gesture-handler": "~2.24.0",
        "react-native-reanimated": "~3.17.4",
        "react-native-safe-area-context": "5.4.0",
        "react-native-screens": "~4.11.1",
        "react-native-web": "^0.20.0"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@react-native-community/cli": "^16.0.0",
        "@types/gh-pages": "^6",
        "@types/jest": "^29.5.12",
        "@types/react": "~19.0.10",
        "@types/react-test-renderer": "^18.0.7",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-universe": "^12.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.34.1",
        "gh-pages": "^6.2.0",
        "jest": "^29.2.1",
        "jest-expo": "~53.0.9",
        "prettier": "^3.2.5",
        "react-test-renderer": "18.3.1",
        "typescript": "~5.8.3"
    },
    "private": true,
    "expo": {
        "autolinking": {
            "nativeModulesDir": "../packages/"
        }
    }
}
