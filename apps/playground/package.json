{
    "name": "audio-playground",
    "version": "0.1.0",
    "main": "src/index",
    "scripts": {
        "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=development expo start --dev-client --port 7365",
        "android": "NODE_ENV=development expo run:android --port 7365",
        "ios": "NODE_ENV=development expo run:ios --device --port 7365",
        "web": "NODE_ENV=development expo start --web --port 7365 --dev-client",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "lint": "eslint src",
        "clean": "rimraf dist",
        "deploy": "yarn clean && NODE_ENV=production expo export -p web && gh-pages -t -d dist --dest playground",
        "predeploy": "expo export -p web",
        "serve:dev": "yarn clean && NODE_ENV=development expo export -p web && yarn serve dist/"
    },
    "dependencies": {
        "@expo/config": "~9.0.0",
        "@expo/metro-runtime": "~3.2.1",
        "@expo/vector-icons": "^14.0.0",
        "@gorhom/bottom-sheet": "5.0.0-alpha.9",
        "@react-navigation/native": "^6.1.17",
        "@shopify/react-native-skia": "1.2.3",
        "@simform_solutions/react-native-audio-waveform": "^1.0.1",
        "@siteed/design-system": "^0.9.0",
        "@siteed/expo-audio-stream": "workspace:^",
        "@siteed/expo-audio-ui": "workspace:^",
        "@siteed/react-native-logger": "^0.9.3",
        "@xenova/transformers": "^2.17.2",
        "buffer": "^6.0.3",
        "debug": "^4.3.4",
        "expo": "^51.0.24",
        "expo-av": "~14.0.5",
        "expo-build-properties": "~0.12.4",
        "expo-clipboard": "~6.0.3",
        "expo-constants": "~16.0.2",
        "expo-document-picker": "~12.0.2",
        "expo-file-system": "~17.0.1",
        "expo-linking": "~6.3.1",
        "expo-localization": "~15.0.3",
        "expo-router": "~3.5.20",
        "expo-sharing": "~12.0.1",
        "expo-status-bar": "~1.12.1",
        "is-base64": "^1.1.0",
        "lodash": "^4.17.21",
        "metro-react-native-babel-transformer": "^0.77.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "0.74.3",
        "react-native-gesture-handler": "~2.16.1",
        "react-native-paper": "^5.12.3",
        "react-native-quick-base64": "^2.1.2",
        "react-native-reanimated": "~3.10.1",
        "react-native-safe-area-context": "4.10.5",
        "react-native-screens": "3.31.1",
        "react-native-svg": "15.2.0",
        "react-native-vector-icons": "^10.0.3",
        "react-native-web": "~0.19.6"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@types/gh-pages": "^6",
        "@types/is-base64": "^1.1.3",
        "@types/lodash": "^4",
        "@types/node": "^20.12.7",
        "@types/react": "~18.2.45",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-universe": "^12.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.34.1",
        "gh-pages": "^6.1.1",
        "prettier": "^3.2.5",
        "rimraf": "^6.0.1",
        "serve": "^14.2.3",
        "ts-node": "^10.9.2",
        "typescript": "~5.3.3"
    },
    "private": true,
    "expo": {
        "autolinking": {
            "nativeModulesDir": "../../packages/expo-audio-stream/"
        }
    },
    "packageManager": "yarn@4.2.2"
}
