{
    "name": "audio-playground",
    "version": "0.1.0",
    "main": "src/index",
    "scripts": {
        "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=development expo start --dev-client --port 7365",
        "android": "NODE_ENV=development expo run:android --port 7365",
        "ios": "RCT_METRO_PORT=7365 NODE_ENV=development expo run:ios --device --port 7365",
        "web": "NODE_ENV=development expo start --web --port 7365 --dev-client",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "lint": "eslint src",
        "clean": "rimraf dist",
        "deploy": "yarn clean && NODE_ENV=production expo export -p web && gh-pages -t -d dist --dest playground",
        "predeploy": "expo export -p web",
        "serve:dev": "yarn clean && NODE_ENV=development expo export -p web && yarn serve dist/"
    },
    "dependencies": {
        "@expo/config": "^10.0.0",
        "@expo/metro-runtime": "~4.0.0",
        "@expo/vector-icons": "^14.0.0",
        "@gorhom/bottom-sheet": "^5.0.5",
        "@gorhom/portal": "^1.0.14",
        "@react-native-community/slider": "4.5.5",
        "@react-navigation/native": "^7.0.0",
        "@shopify/react-native-skia": "1.5.0",
        "@siteed/design-system": "^0.31.0",
        "@siteed/expo-audio-stream": "workspace:^",
        "@siteed/expo-audio-ui": "workspace:^",
        "@siteed/react-native-logger": "^0.11.0",
        "@xenova/transformers": "^2.17.2",
        "buffer": "^6.0.3",
        "canvaskit-wasm": "^0.39.1",
        "debug": "^4.3.4",
        "expo": "^52.0.17",
        "expo-av": "~15.0.1",
        "expo-build-properties": "~0.13.1",
        "expo-clipboard": "~7.0.0",
        "expo-constants": "~17.0.3",
        "expo-document-picker": "~13.0.1",
        "expo-file-system": "~18.0.4",
        "expo-linking": "~7.0.3",
        "expo-localization": "~16.0.0",
        "expo-router": "~4.0.11",
        "expo-sharing": "~13.0.0",
        "expo-status-bar": "~2.0.0",
        "is-base64": "^1.1.0",
        "lodash": "^4.17.21",
        "lodash.throttle": "^4.1.1",
        "metro-react-native-babel-transformer": "^0.77.0",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.3",
        "react-native-gesture-handler": "~2.20.2",
        "react-native-localize": "^3.2.1",
        "react-native-paper": "^5.12.3",
        "react-native-paper-dates": "^0.22.26",
        "react-native-quick-base64": "^2.1.2",
        "react-native-reanimated": "~3.16.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "~4.1.0",
        "react-native-svg": "15.8.0",
        "react-native-vector-icons": "^10.0.3",
        "react-native-web": "~0.19.13",
        "whisper.rn": "^0.3.9"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/plugin-transform-private-methods": "^7.25.9",
        "@babel/plugin-transform-private-property-in-object": "^7.25.9",
        "@types/gh-pages": "^6",
        "@types/is-base64": "^1.1.3",
        "@types/lodash": "^4",
        "@types/lodash.throttle": "^4",
        "@types/node": "^22.7.5",
        "@types/react": "~18.3.12",
        "@typescript-eslint/eslint-plugin": "^7.7.0",
        "@typescript-eslint/parser": "^7.7.0",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-universe": "^12.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-react": "^7.34.1",
        "gh-pages": "^6.1.1",
        "prettier": "^3.2.5",
        "rimraf": "^6.0.1",
        "serve": "^14.2.3",
        "ts-node": "^10.9.2",
        "typescript": "~5.3.3"
    },
    "private": true,
    "expo": {
        "autolinking": {
            "nativeModulesDir": "../../packages/expo-audio-stream/"
        }
    },
    "packageManager": "yarn@4.2.2"
}
