        "@babel/preset-react": "^7.27.1",
        "@config-plugins/detox": "^9.0.0",
        "@expo/config": "~12.0.12",
        "@expo/config-plugins": "~54.0.4",
        "@playwright/test": "^1.50.0",
        "@storybook/addon-links": "^9.0.9",
        "@storybook/addon-ondevice-actions": "^9.0.6",
        "@storybook/addon-ondevice-controls": "^9.0.6",
        "@storybook/addon-react-native-web": "^0.0.29",
        "@storybook/addon-webpack5-compiler-swc": "^3.0.0",
        "@storybook/jest": "^0.2.3",
        "@storybook/react-native": "^9.0.6",
        "@storybook/react-webpack5": "^9.0.9",
        "@storybook/test-runner": "^0.23.0",
        "@testing-library/jest-native": "^5.4.3",
        "@testing-library/react-native": "^13.2.0",
        "@types/gh-pages": "^6",
        "@types/is-base64": "^1.1.3",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4",
        "@types/lodash.throttle": "^4",
        "@types/node": "^22.7.5",
        "@types/react": "~19.1.10",
        "@types/react-test-renderer": "^19",
        "@typescript-eslint/eslint-plugin": "^8.18.1",
        "@typescript-eslint/parser": "^8.18.1",
        "babel-loader": "^10.0.0",
        "babel-plugin-module-resolver": "^5.0.2",
