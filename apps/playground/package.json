{
    "name": "audio-playground",
    "version": "0.9.0",
    "main": "src/index",
    "scripts": {
        "eas-build-pre-install": "corepack enable && cd ../.. && yarn install && cd packages/expo-audio-stream && yarn build && yarn build:plugin",
        "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 NODE_ENV=development expo start --dev-client --port 7365",
        "android": "RCT_METRO_PORT=7365 NODE_ENV=development expo run:android --port 7365",
        "ios": "RCT_METRO_PORT=7365 NODE_ENV=development expo run:ios --device --port 7365",
        "web": "NODE_ENV=development expo start --web --port 7365 --dev-client",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "lint": "eslint src",
        "clean": "rimraf dist",
        "build:android:test": "APP_VARIANT=development eas build --platform android --profile test --local",
        "build:android:development": "APP_VARIANT=development eas build --platform android --profile development --local",
        "build:android:preview": "APP_VARIANT=preview eas build --platform android --profile preview --local",
        "build:android:preview_apk": "APP_VARIANT=preview_apk eas build --platform android --profile preview_apk --local",
        "build:android:production": "NODE_ENV=production APP_VARIANT=production eas build --platform android --profile production",
        "build:ios:test": "APP_VARIANT=development eas build --platform ios --profile test --local",
        "build:ios:development": "APP_VARIANT=development NODE_ENV=development eas build --platform ios --profile development --local",
        "build:ios:preview": "APP_VARIANT=preview NODE_ENV=development eas build --platform ios --profile preview --local",
        "build:ios:production": "NODE_ENV=production APP_VARIANT=production eas build --platform ios --profile production",
        "deploy": "yarn clean && NODE_ENV=production APP_VARIANT=production EXPO_WEB=true expo export -p web && cp playstore_policy.html dist/ && gh-pages -t -d dist --dest playground",
        "predeploy": "APP_VARIANT=production EXPO_WEB=true expo export -p web",
        "serve:dev": "yarn clean && NODE_ENV=development APP_VARIANT=development EXPO_WEB=true expo export -p web && yarn serve dist/"
    },
    "dependencies": {
        "@expo/cli": "^0.22.3",
        "@expo/metro-runtime": "~4.0.1",
        "@expo/vector-icons": "^14.0.0",
        "@gorhom/bottom-sheet": "^5.0.5",
        "@gorhom/portal": "^1.0.14",
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-native-community/slider": "4.5.5",
        "@react-native/assets-registry": "^0.76.5",
        "@react-navigation/native": "^7.0.0",
        "@reduxjs/toolkit": "^2.5.0",
        "@shopify/react-native-skia": "1.5.0",
        "@siteed/design-system": "^0.45.0",
        "@siteed/expo-audio-stream": "workspace:^",
        "@siteed/expo-audio-ui": "workspace:^",
        "@siteed/react-native-logger": "^0.11.0",
        "@xenova/transformers": "^2.17.2",
        "buffer": "^6.0.3",
        "canvaskit-wasm": "^0.39.1",
        "crc-32": "^1.2.2",
        "debug": "^4.3.4",
        "expo": "^52.0.37",
        "expo-application": "~6.0.2",
        "expo-av": "~15.0.2",
        "expo-build-properties": "~0.13.2",
        "expo-clipboard": "~7.0.1",
        "expo-constants": "~17.0.7",
        "expo-device": "~7.0.2",
        "expo-document-picker": "~13.0.3",
        "expo-file-system": "~18.0.11",
        "expo-linking": "~7.0.5",
        "expo-localization": "~16.0.1",
        "expo-router": "4.0.14",
        "expo-sharing": "~13.0.1",
        "expo-status-bar": "~2.0.1",
        "expo-updates": "~0.27.1",
        "is-base64": "^1.1.0",
        "lodash": "^4.17.21",
        "lodash.throttle": "^4.1.1",
        "metro-react-native-babel-transformer": "^0.77.0",
        "onnxruntime-react-native": "^1.20.1",
        "onnxruntime-web": "^1.20.1",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-native": "0.76.7",
        "react-native-gesture-handler": "~2.20.2",
        "react-native-localize": "^3.2.1",
        "react-native-paper": "^5.13.1",
        "react-native-paper-dates": "^0.22.26",
        "react-native-quick-base64": "^2.1.2",
        "react-native-reanimated": "~3.16.2",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "~4.4.0",
        "react-native-svg": "15.8.0",
        "react-native-vector-icons": "^10.0.3",
        "react-native-web": "~0.19.13",
        "react-redux": "^9.2.0",
        "redux-persist": "^6.0.0",
        "whisper.rn": "^0.4.0-rc.10"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/plugin-proposal-decorators": "^7.25.9",
        "@babel/plugin-syntax-import-meta": "^7.10.4",
        "@babel/plugin-transform-private-methods": "^7.25.9",
        "@babel/plugin-transform-private-property-in-object": "^7.25.9",
        "@config-plugins/detox": "^9.0.0",
        "@expo/config": "^10.0.6",
        "@expo/config-plugins": "~9.0.0",
        "@types/gh-pages": "^6",
        "@types/is-base64": "^1.1.3",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4",
        "@types/lodash.throttle": "^4",
        "@types/node": "^22.7.5",
        "@types/react": "~18.3.12",
        "@typescript-eslint/eslint-plugin": "^8.18.1",
        "@typescript-eslint/parser": "^8.18.1",
        "babel-plugin-module-resolver": "^5.0.2",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "detox": "^20.34.4",
        "dotenv-flow": "^4.1.0",
        "env-cmd": "^10.1.0",
        "eslint": "^8.56.0",
        "eslint-config-expo": "^8.0.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-promise": "^7.2.1",
        "expo-dev-client": "~5.0.12",
        "gh-pages": "^6.2.0",
        "jest": "^29",
        "joi": "^17.13.3",
        "prettier": "^3.4.2",
        "prettier-eslint": "^16.3.0",
        "rimraf": "^6.0.1",
        "serve": "^14.2.3",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "typescript": "~5.3.3"
    },
    "private": true,
    "expo": {
        "autolinking": {
            "nativeModulesDir": "../../packages/expo-audio-stream/"
        }
    },
    "packageManager": "yarn@4.2.2"
}
