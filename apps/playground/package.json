    "private": true,
    "expo": {
        "autolinking": {
            "nativeModulesDir": "../packages/"
        }
    },
    "packageManager": "yarn@4.2.2",
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
