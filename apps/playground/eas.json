{
  "cli": {
    "version": ">= 14.1.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "channel": "development",
      "developmentClient": true,
      "env": {
        "APP_VARIANT": "development",
        "NPM_AUTH_TOKEN": "NONE",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "YARN_ENABLE_GLOBAL_CACHE": "false"
      },
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "buildArtifactPaths": ["android/app/build/outputs/apk/debug/AudioDevPlayground-debug.apk"]
      },
      "ios": {
        "buildConfiguration": "Debug",
        "buildArtifactPaths": ["ios/build/Build/Products/Debug-iphonesimulator/AudioDevPlayground.app"]
      },
      "cache": {
        "paths": [
          "~/.yarn/cache"
        ]
      }
    },
    "test": {
      "android": {
        "gradleCommand": ":app:assembleRelease :app:assembleAndroidTest -DtestBuildType=release",
        "withoutCredentials": true
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_VARIANT": "test",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "YARN_ENABLE_GLOBAL_CACHE": "false",
        "NPM_AUTH_TOKEN": "NONE"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": true,
        "buildArtifactPaths": ["ios/build/Build/Products/Release-iphonesimulator/AudioPlayground.app"]
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "preview",
        "NPM_AUTH_TOKEN": "NONE",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "YARN_ENABLE_GLOBAL_CACHE": "false"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_VARIANT": "production",
        "NPM_AUTH_TOKEN": "NONE",
        "NODE_ENV": "production",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "YARN_ENABLE_GLOBAL_CACHE": "false"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}