        "directory": "packages/expo-audio-ui"
    },
    "bugs": {
        "url": "https://github.com/deeeed/expo-audio-stream/issues"
    },
    "keywords": [
        "react-native",
        "expo",
        "expo-audio-stream",
        "ExpoAudioStream"
    ],
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "scripts": {
