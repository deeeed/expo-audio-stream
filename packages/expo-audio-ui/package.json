        "eslint-plugin-react": "^7.34.4",
        "expo": "^51.0.21",
        "globals": "^15.8.0",
        "react-native": "^0.74.3",
        "react-native-gesture-handler": "2.16.1",
        "react-native-reanimated": "3.10.1",
        "rimraf": "^6.0.1",
        "rollup": "^4.18.1",
        "size-limit": "^11.1.4",
        "typescript": "^5.5.3",
        "typescript-eslint": "^7.16.1"
