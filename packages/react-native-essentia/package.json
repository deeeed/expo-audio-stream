{
  "name": "@siteed/react-native-essentia",
  "version": "0.3.1",
  "description": "RN jni bindings for essentia",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "react-native.config.js",
    "install.js",
    "setup.sh",
    "build-essentia-ios.sh",
    "build-essentia-android.sh",
    "build-all.sh",
    "publish-static-libs.sh",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli android/build example/android/build example/android/app/build example/ios/build lib",
    "prepare": "bob build",
    "publish-beta": "npm publish --tag beta",
    "setup": "./setup.sh",
    "build:ios": "./build-essentia-ios.sh",
    "build:ios:force": "./build-essentia-ios.sh --force",
    "build:android": "./build-essentia-android.sh",
    "build:android:force": "./build-essentia-android.sh --force",
    "build:all": "./build-all.sh",
    "build:all:force": "./build-all.sh --force",
    "postinstall": "node ./install.js",
    "publish-static": "./publish-static-libs.sh"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeeed/expo-audio-stream/.git"
  },
  "author": "Arthur Breton <abreton@siteed.net> (https://github.com/deeeed)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deeeed/expo-audio-stream//issues"
  },
  "homepage": "https://github.com/deeeed/expo-audio-stream/#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "nativePackage": true,
  "codegenConfig": {
    "name": "RNEssentiaSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "node-fetch": "2",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.73.1",
    "@siteed/publisher": "^0.6.0",
    "@types/adm-zip": "^0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/jest": "^29.5.5",
    "@types/progress": "^2",
    "@types/react": "^18.2.44",
    "commitlint": "^17.0.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "19.0.0",
    "react-native": "0.78.0",
    "react-native-builder-bob": "^0.37.0",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json",
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "legacy-module",
    "languages": "cpp",
    "version": "0.48.3"
  }
}
